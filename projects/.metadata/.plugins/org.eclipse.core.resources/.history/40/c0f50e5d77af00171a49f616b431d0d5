package practice;

import java.util.Scanner;

public class ArrayDemoe {

	public static void main(String[] args) {
		boolean pay=true;
		int n;
		String[] s=null;
		System.out.println("enter number of students");
		Scanner sc = new Scanner(System.in);
		n=sc.nextInt();
		for(int i=0;i<=n;i++)
		{
			System.out.println("Enter" +i+ "Student name" );
			Scanner sc1 = new Scanner(System.in);
			s[i]=sc1.nextLine();
		}
		
		for(int i = 1;i<n;i++)
		{
			if(pay==true)
			{
				System.out.println("student is paid");
		 }
			else
			{
				System.out.println("student is  not paid");
			}
		
		
		}
		
	}
}
