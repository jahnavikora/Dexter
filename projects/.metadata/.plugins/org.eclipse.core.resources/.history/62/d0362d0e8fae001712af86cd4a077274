package practice;

public class InheritanceDemo {

	

	public static void main(String[] args) {
		
		Dog d = new Dog("Tommy", 5, );
		
		
		
		d.eat();
		d.walk();
		d.GiveBirth();
		
		Cow c = new Cow("bimmy",10);
		c.eat();
		c.walk();
		c.GiveBirth();
		c.milk();
		
		Cat cat = new Cat("Jackie", 7);
		cat.eat();
		cat.walk();
		cat.GiveBirth();
		
		
		
		
	
		
		
				

	}

}
